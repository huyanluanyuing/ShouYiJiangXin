link :to=\"{ name: 'FrontHome' }\" style=\"position: absolute; left: 420px; top: 130px;\">\n                        <el-button class=\"primary-btn\">再现非遗</el-button>\n                    </router-link>\n                  <router-link :to=\"{ name: 'feiyiMain' }\" style=\"position: absolute; left: 590px; top: 130px;\">\n                    <el-button class=\"primary-btn\">特色非遗</el-button>\n                  </router-link>\n\n                </div>\n\n                <div class=\"logo_big w\"></div>\n            </div>\n        </full-page>\n    </div>\n\n</template>\n  \n<script>\nimport * as echarts from 'echarts'\nimport \"../fonts/FZQKBYSJW.css\";\n\nexport default {\n    name: \"Prepare\",\n    data() {\n        return {\n            options: {\n                navigation: true,\n            },\n        }\n    },\n\n    mounted() {\n      (function () {\n        var myChart = echarts.init(document.querySelector(\".chart_loading\"));\n        var option = {\n          graphic: {\n            elements: [\n              {\n                type: 'group',\n                left: 'center',\n                top: '20',\n                children: new Array(1).fill(0).map((val, i) => ({\n                  type: 'circle', // 使用圆形元素\n                  x: i * 20,\n                  shape: {\n                    cx: 0, // 圆心的x坐标\n                    cy: 0, // 圆心的y坐标\n                    r: 45 // 圆的半径\n                  },\n                  style: {\n                    fill: '#DC143C'\n                  },\n                  keyframeAnimation: [\n                    {\n                      duration: 3500,\n                      loop: true,\n                      keyframes: [\n                        {\n                          percent: 0.5,\n                          easing: 'sinusoidalInOut',\n                          scaleX: 0.1,\n                          scaleY: 0.1\n                        },\n                        {\n                          percent: 1,\n                          easing: 'sinusoidalInOut',\n                          scaleX: 1,\n                          scaleY: 1\n                        }\n                      ]\n                    }\n                  ]\n                }))\n              }\n            ]\n          }\n        };\n        myChart.setOption(option, true);\n        window.addEventListener('resize', function () {\n          myChart.resize();\n        });\n      })();\n\n\n        (function () {\n            var myChart = echarts.init(document.querySelector(\".chart_name\"));\n            var option = {\n                graphic: {\n                    elements: [\n                        {\n                            type: 'text',\n                            left: '30px',\n                            top: '20px',\n                            style: {\n                                text: '守艺匠心',\n                                fontSize: 80,\n                                fontWeight: 'bold',\n                                lineDash: [0, 200],\n                                lineDashOffset: 0,\n                                fill: 'transparent',\n                                stroke: '#ffffff',\n                                lineWidth: 1\n                            },\n                            keyframeAnimation: {\n                                duration: 3000,\n                                loop: false,\n                                keyframes: [\n                                    {\n                                        percent: 0.7,\n                                        style: {\n                                            fill: 'transparent',\n                                            lineDashOffset: 200,\n                                            lineDash: [200, 0]\n                                        }\n                                    },\n                                    {\n                                        // Stop for a while.\n                                        percent: 0.8,\n                                        style: {\n                                            fill: 'transparent'\n                                        }\n                                    },\n                                    {\n                                        percent: 1,\n                                        style: {\n                                            fill: '#ffffff'\n                                        }\n                                    }\n                                ]\n                            }\n                        }\n                    ]\n                }\n            };\n            myChart.setOption(option, true);\n            window.addEventListener('resize', function () {\n                myChart.resize();\n            });\n        })();\n    }\n}\n</script>\n  \n<style scoped>\nbody {\n    margin: 0;\n    padding: 0;\n}\n\n.w {\n    width: 1210px;\n    margin: auto;\n}\n\n.header {\n    height: 30px;\n    margin: 0px auto;\n    margin-top: 30px;\n}\n\n.name {\n    float: left;\n    font-size: 25px;\n    line-height: 30px;\n}\n\n.wrapper {\n    position: relative;\n    height: 100vh;\n    //background-image: linear-gradient(to bottom right, #C8E5DA, #EDF5F5);\n    background-image: url(../assets/bg.png);\n    background-size: 100%;\n    overflow: hidden;\n}\n\n.bg {\n    position: absolute;\n    left: 0%;\n    bottom: 0%;\n    width: 1630px;\n    height: 595px;\n    opacity: 0.6;\n    background-size: 100%;\n    background-repeat: no-repeat;\n}\n\n.logo {\n    float: left;\n    width: 60px;\n    height: 60px;\n    margin-top: -16px;\n    background-image: url(../assets/logo.png);\n    background-repeat: no-repeat;\n    background-position-x: center;\n    background-size: 240%;\n}\n\n.chart {\n    float: left;\n    height: 200px;\n}\n\n.logo_big {\n    position: absolute;\n    right: 180px;\n    top: -30px;\n    width: 300px;\n    height: 300px;\n    background-repeat: no-repeat;\n    background-position-x: center;\n    background-size: 280%;\n}\n\n.primary-btn {\n  z-index: 100;\n  display: block;\n  position: relative;\n  left: 10px;\n  width: 150px;\n  height: 40px;\n  /* 移除 border-radius 属性 */\n  /* border-radius: 20px; */\n  margin-top: 0px;\n  text-align: center;\n  font-size: 14px;\n  letter-spacing: 2px;\n  background-color: #bc3454;\n  color: #f9f9f9;\n  cursor: pointer;\n  /* 添加边框属性 */\n  border: 2px solid #bc3454;\n}\n\n.primary-btn:hover {\n  box-shadow: 2px 2px 6px 0 rgba(255, 255, 255, 0.6),\n  -2px -2px 6px 0 rgba(116, 125, 136, 0.5),\n  inset -4px -4px 3px 0 rgba(255, 255, 255, 0.2),\n  inset 4px 4px 3px 0 rgba(0, 0, 0, 0.4);\n  /* 移除原有的边框属性 */\n  border: none;\n}\n.feature {\n    width: 95%;\n    height: 450px;\n}\n\n.feature:hover {\n    transition: transform 0.5s ease-out;\n    transform: translate(0px, -30px);\n}\n\n.font_fzqkbysjt {\n    font-family: STKAITI;\n    color: white;\n}\n</style>\n  "]}]}