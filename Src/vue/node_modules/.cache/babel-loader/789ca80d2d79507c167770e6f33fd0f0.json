ladon'})\n    },\n    // 初始化数据项总数\n    initializeItemCount() {\n      this.itemCount = this.feiyiLeftList.length;\n    },\n    // 启动滚动\n    startScroll() {\n      this.initializeItemCount();\n      console.log(\"object\");\n      setInterval(() => {\n        // 每隔一段时间更新当前显示的数据项索引\n        this.currentIndex = (this.currentIndex + 1) % this.itemCount;\n      }, 3000); // 3秒滚动一次，可以根据需要调整时间间隔\n    },\n\n    handleClick_xihuan(tab, event) {\n      console.log(tab, event);\n    },\n    handleSizeChange(val) {\n      console.log(`每页 ${val} 条`);\n    },\n    handleCurrentChange(val) {\n      console.log(`当前页: ${val}`);\n    },\n    next() {\n      // 向下滚动一页\n      this.$refs.fullpage.api.moveSectionDown();\n    }\n  }\n}\n</script>\n\n<style scoped>\n.midtitle{\n  font-family: '华文楷体';\n  font-size: 30px;\n  color: #7fac9b;\n  font-weight: bold;\n  text-align: left;\n  margin-top:0px;\n}\n.leftList .title{\n  font-family: '华文楷体';\n  font-size: 30px;\n  color: #7fac9b;\n  font-weight: bold;\n  text-align: center;\n}\n.righttitle{\n  font-family: '华文楷体';\n  font-size: 30px;\n  color: #7fac9b;\n  font-weight: bold;\n  text-align: center;\n  margin-bottom: 10px;\n\n  margin-top: 10px;\n}\n .seamless-warp {\n        height: 1000px;\n        overflow: hidden;\n    }\n.slide-right-enter-active, .slide-right-leave-active {\n  transition: all 1s; /* 过渡时间可以根据需要调整 */\n}\n.slide-right-enter, .slide-right-leave-to {\n  transform: translateX(100%);\n}\n.right{\n  /* background: red; */\n  width: 100%;\n  /* height:1000px; */\n  /* overflow: hidden; */\n}\n.right .image-card{\n  display: inline-block;\n  box-sizing: border-box;\n  width: 100%;\n  /* height:200px; */\n  padding: 10px;\n}\n.right .image-card img{\n  width: 100%;\n  height: 300px;\n  object-fit: cover;\n  border-radius: 10px;\n}\n.right .card-text{\n  color: rgb(47, 47, 47);\n  font-size: 18px;\n  font-family: '华文楷体', cursive;\n  text-align: center; /* 水平居中文本 */\n}\n.card-text-up{\n  color: rgb(35, 34, 34);\n  font-size: 10px