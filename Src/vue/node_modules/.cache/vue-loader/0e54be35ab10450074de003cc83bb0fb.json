{"remainingRequest":"C:\\Users\\15462\\Desktop\\xiang-hua-master\\xiang-hua-master\\Src\\vue\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!C:\\Users\\15462\\Desktop\\xiang-hua-master\\xiang-hua-master\\Src\\vue\\src\\views\\front\\liaoning.vue?vue&type=style&index=0&id=e9e4b642&scoped=true&lang=css&","dependencies":[{"path":"C:\\Users\\15462\\Desktop\\xiang-hua-master\\xiang-hua-master\\Src\\vue\\src\\views\\front\\liaoning.vue","mtime":1696000587632},{"path":"C:\\Users\\15462\\Desktop\\xiang-hua-master\\xiang-hua-master\\Src\\vue\\node_modules\\css-loader\\dist\\cjs.js","mtime":1695379667140},{"path":"C:\\Users\\15462\\Desktop\\xiang-hua-master\\xiang-hua-master\\Src\\vue\\node_modules\\vue-loader\\lib\\loaders\\stylePostLoader.js","mtime":1695379668414},{"path":"C:\\Users\\15462\\Desktop\\xiang-hua-master\\xiang-hua-master\\Src\\vue\\node_modules\\postcss-loader\\src\\index.js","mtime":1695379667973},{"path":"C:\\Users\\15462\\Desktop\\xiang-hua-master\\xiang-hua-master\\Src\\vue\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1695379654940},{"path":"C:\\Users\\15462\\Desktop\\xiang-hua-master\\xiang-hua-master\\Src\\vue\\node_modules\\vue-loader\\lib\\index.js","mtime":1695379668414}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["liaoning.vue"],"names":[],"mappings":";;AA4jBA;AACA;AACA;AACA;;;AAGA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;;AAEA;AACA;AACA;;;AAGA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA","file":"liaoning.vue","sourceRoot":"src/views/front","sourcesContent":["<template>\n  <div class=\"wrapper\">\n    <div style=\"width: 100%; heigh: 100%\">\n      <section class=\"mainbox\">\n        <div class=\"column bgClass\">\n          <div class=\"province\" style=\"font-family: '华文楷体';font-weight: bold\">{{ name }}</div>\n          <div class=\"map_info\">\n          <div class=\"province_info\" style=\"font-family: '华文楷体'\">{{ intro }}</div>\n          </div>\n          <div class=\"map\">\n            <div\n                id=\"main\"\n                style=\"margin: -20px -90px; width: 950px; height: 650px\"\n            ></div>\n            <div class=\"info\" style=\"font-family: '华文楷体'\">审图号: GS京(2022)1061号</div>\n          </div>\n\n          <div\n              class=\"map_explaination\"\n              style=\"\n              position: absolute;\n              left: 20px;\n              bottom: 20px;\n              font-size: 14px;\n              color: #030303;\n              font-family: '华文楷体';\n            \"\n          ></div>\n        </div>\n\n        <div class=\"column\">\n          <div class=\"no\">\n            <dv-border-box-9>\n              <div class=\"no-hd\">\n                <ul>\n                  <li class=\"li1\" style=\"font-family: '华文楷体'\">76</li>\n                  <li style=\"font-family: '华文楷体'\">58</li>\n                </ul>\n              </div>\n              <div class=\"no-bd\">\n                <ul>\n                  <li style=\"font-size: 17px; font-family: '华文楷体';font-weight: bold\">国家级非遗代表性项目</li>\n                  <li style=\"font-size: 17px; font-family: '华文楷体';font-weight: bold\">国家级非遗代表性传承人</li>\n                </ul>\n              </div>\n              <div class=\"no_footer\"></div>\n            </dv-border-box-9>\n          </div>\n          <div class=\"panel bar2\">\n            <h2 style=\"font-family: '华文楷体';font-weight: bold\">辽宁省各类国家级非遗数量</h2>\n            <div class=\"chart\" style=\"margin-top: -55px; height: 310px !important\">图表</div>\n            <div class=\"panel-footer\"></div>\n          </div>\n          <div class=\"panel bar\">\n            <h2 style=\"font-family: '华文楷体';font-weight: bold\">辽宁省各类国家级非遗传承人数量</h2>\n            <div class=\"chart\" style=\"font-family: '华文楷体'\">图表</div>\n            <div class=\"panel-footer\"></div>\n          </div>\n        </div>\n      </section>\n\n      <div>\n        <div class=\"province\" style=\"font-family: '华文楷体';font-weight: bold\">特色非遗</div>\n\n        <div class=\"huapian_item\">\n          <img src=\"../../assets/huapian/金州舞龙.png\" />\n\n          <h2 style=\"font-family: '华文楷体'\">金州龙舞</h2>\n          <p style=\"font-family: '华文楷体';font-weight: bold\">\n            龙舞（金州龙舞），辽宁省大连市金州区传统舞蹈，国家级非物质文化遗产之一。<br />\n            金州龙舞又称“西门外龙舞”，流行于辽宁省大连市金州地区。金州龙舞多在农闲、节庆时表演，动作变化多端，已成为带有辽南地区特色的民间舞蹈形式。<br />\n            表演中，一支舞龙队由两条龙和一个龙珠组成，共19人，舞姿变化多端，花样近40种。伴奏音乐主要运用东北秧歌的曲牌。龙的形象剽悍俊美，龙的动作的配合展现出中华民族的气质，并有着浓郁的乡土气息。\n            舞姿变化多端，神奇而富有魅力。<br />\n          </p>\n          <div\n              style=\"\n              display: inline-block;\n              position: absolute;\n              margin-top: 160px;\n              width: 340px;\n            \"\n          >\n            <P style=\"color: #000; margin-top: 90px; font-family: '华文楷体';font-weight: bold\">其他非遗：抚顺地秧歌</P>\n            <img\n                src=\"../../assets/feiyi/抚顺地秧歌.png\"\n                style=\"\n                width: 320px;\n                height: 180px;\n                margin: 125px 20px;\n                object-fit: cover;\n              \"\n            />\n          </div>\n          <div\n              style=\"\n              display: inline-block;\n              position: absolute;\n              margin-top: 160px;\n              margin-left: 380px;\n              width: 340px;\n            \"\n          >\n            <P style=\"color: #000; margin-top: 90px; font-family: '华文楷体';font-weight: bold\">其他非遗：东北二人转</P>\n            <img\n                src=\"../../assets/feiyi/二人转.png\"\n                style=\"\n                width: 320px;\n                height: 180px;\n                margin: 125px 20px;\n                object-fit: cover;\n              \"\n            />\n          </div>\n        </div>\n\n        <div class=\"huapian_item\">\n          <img src=\"../../assets/huapian/长海号子.png\" />\n\n          <h2 style=\"font-family: '华文楷体'\">长海号子</h2>\n          <p style=\"font-family: '华文楷体';font-weight: bold\">\n            长海号子，又称海洋号子，是辽宁省长海县的传统音乐，也是国家级非物质文化遗产之一。\n            它分为两大类：船民号子和渔民号子。\n            船民号子伴随运输船上的劳动呼喊，而渔民号子则主要用于渔民打鱼时的喊唱。<br />\n            这些号子的曲调各具特色，包括四平腔、二六腔、流水腔、散腔等多种音调。\n            号子的节奏生动明快，通常采用4/4、2/4、1/4拍和散板，歌词简洁通俗，由三至五个字组成。<br />\n            著名的长海号子包括撑大篷号子、拔锚号子、勒锚号子、摇橹号子等。\n            这些号子传承了地方文化，以其独特风格和丰富表现力而著称。\n          </p>\n          <div\n              style=\"\n              display: inline-block;\n              position: absolute;\n              margin-top: 160px;\n              width: 340px;\n            \"\n          >\n            <P style=\"color: #000; margin-top: 90px; font-family: '华文楷体';font-weight: bold\">其他非遗：唢呐艺术</P>\n            <img\n                src=\"../../assets/feiyi/唢呐艺术.png\"\n                style=\"\n                width: 320px;\n                height: 180px;\n                margin: 125px 20px;\n                object-fit: cover;\n              \"\n            />\n          </div>\n          <div\n              style=\"\n              display: inline-block;\n              position: absolute;\n              margin-top: 160px;\n              margin-left: 380px;\n              width: 340px;\n            \"\n          >\n            <P style=\"color: #000; margin-top: 90px; font-family: '华文楷体';font-weight: bold\">其他非遗：辽宁鼓乐</P>\n            <img\n                src=\"../../assets/feiyi/鼓乐.png\"\n                style=\"\n                width: 320px;\n                height: 180px;\n                margin: 125px 20px;\n                object-fit: cover;\n              \"\n            />\n          </div>\n        </div>\n      </div>\n    </div>\n\n  </div>\n</template>\n\n\n<script>\n\nimport * as echarts from \"echarts\";\nimport \"../../js/flexible\";\nconst sichuanJson = require(\"../../assets/liaoning.json\"); //获取中国地图的json数据\nimport \"../../fonts/DS-DIGIT.css\";\nimport { borderBox9} from '@jiaminghi/data-view'\nimport request from \"@/utils/request\";\nimport \"../../fonts/FZQKBYSJW.css\";\nexport default {\n  name: \"huapianMain\",\n  data() {\n    return {\n      name: \"辽宁省\",\n      intro:\n        \"辽宁省，简称“辽”，是中华人民共和国东北地区的省级行政区，省会沈阳，位于中国东北边疆地区，濒临渤海和黄海，具有丰富多彩的历史文化和自然风光。辽宁省拥有悠久的历史，曾是中国东北满族建立的清朝的发源地，因此也被誉为“清东陵”。辽宁省有着传统的锦绣文化，以及著名的盛京文化，包括豫园、沈阳故宫等历史遗迹。\",\n    };\n  },\n    components: {\n        borderBox9\n    },\n  methods: {},\n\n  mounted() {\n    //柱状图-左侧\n    (function () {\n      var myChart = echarts.init(document.querySelector(\".bar .chart\"));\n      var timer = null;\n      myChart.clear();\n      clearInterval(timer);\n      var SData = [4, 8, \n      8, 14, 11, 0, 8, 2, 1, 2];\n      var option = {\n    color: [\"#34bca3\", \"#0099cc\", \"#ff9900\", \"#ff6600\", \"#ff33cc\", \"#9933cc\", \"#669900\", \"#336699\", \"#993333\", \"#996633\"],\n    tooltip: {\n        trigger: \"axis\",\n        axisPointer: {\n            type: \"shadow\",\n        },\n    },\n    dataZoom: [\n        {\n            id: \"dataZoomX\",\n            show: false,\n            type: \"slider\",\n            xAxisIndex: [0],\n            filterMode: \"filter\",\n            startValue: 0,\n            endValue: 9,\n        },\n    ],\n    grid: {\n        left: \"0%\",\n        top: \"10px\",\n        right: \"0%\",\n        bottom: \"4%\",\n        containLabel: true,\n    },\n    xAxis: [\n        {\n            type: \"category\",\n            data: [\n            '民间文学',\n            '传统音乐',\n            '传统舞蹈',\n            '传统戏剧',\n            '曲艺',\n            '传统体育、游艺杂技',\n            '传统美术',\n            '传统技艺',\n            '传统医药',\n            '民俗',\n        ],\n            axisLabel: {\n                color: \"#08544c\",\n                fontSize: \"8.5\",\n                interval: 0,\n            },\n            axisLine: {\n                show: false,\n            },\n            axisTick: {\n                show: false,\n            },\n        },\n    ],\n    yAxis: [\n        {\n            type: \"value\",\n            axisLabel: {\n                color: \"#08544c\",\n                fontSize: \"10\",\n                interval: 0,\n            },\n            axisLine: {\n                lineStyle: {\n                    color: \"rgba(255,255,255,0.1)\",\n                    width: 1,\n                    type: \"solid\",\n                },\n            },\n            splitLine: {\n                show: true,\n            },\n        },\n    ],\n    series: [\n        {\n            name: \"数量\",\n            type: \"bar\",\n            barWidth: \"35%\",\n            data: SData,\n            itemStyle: {\n                barBorderRadius: 0,\n                // 颜色区分\n                color: function(params) {\n                    var colorList = [\n                        \"#34bca3\", \"#0099cc\", \"#ff9900\", \"#ff6600\", \"#ff33cc\", \"#9933cc\", \"#669900\",\n                        \"#336699\", \"#993333\", \"#996633\", \"#666666\"\n                    ];\n                    return colorList[params.dataIndex];\n                },\n            },\n            animation: false, // 取消动画\n            label: {\n                show: true,\n                position: \"top\",\n            },\n        },\n    ],\n};\n      myChart.clear();\n      myChart.setOption(option, true);\n      window.addEventListener(\"resize\", function () {\n        myChart.resize();\n      });\n    })();\n\n    //柱状图-右侧\n    (function () {\n        var myColor = [\n  \"#1089E7\",\n  \"#F57474\",\n  \"#56D0E3\",\n  \"#F8B448\",\n  \"#8B78F6\",\n  \"#34BCA3\",\n  \"#FF9900\",\n  \"#FF6666\",\n  \"#66CC99\",\n  \"#FFCC99\"\n];\n      var myChart = echarts.init(document.querySelector(\".bar2 .chart\"));\n      var timer = null;\n      myChart.clear();\n      clearInterval(timer);\n    var SData = [\n        {\n            name: '民间文学',\n            value: 6\n        },\n        {\n            name: '传统音乐',\n            value: 9\n        },\n        {\n            name: '传统舞蹈',\n            value:9\n        },\n        {\n            name: '传统戏剧',\n            value: 10\n        },\n        {\n            name: '曲艺',\n            value:12\n        },\n        {\n            name:'传统体育、游艺与杂技',\n            value: 2\n        },\n        {\n            name:'传统美术',\n            value: 14\n        },\n        {\n            name:'传统技艺',\n            value:4\n        },\n        {\n            name:'传统医药',\n            value: 2\n        },\n        {\n            name:'民俗',\n            value: 8\n        },\n    ];\n\n      var option = {\n        legend: {\n            formatter: function (name) {\n        // 在这里将图例项的名称和对应数值拼接起来\n        var value = 0; // 这里假设初始值为0，您可以根据实际数据获取数值\n        for (var i = 0; i < SData.length; i++) {\n            if (SData[i].name === name) {\n                value = SData[i].value;\n                break;\n            }\n        }\n        return name;\n    },\n        show: true, // 显示legend\n        data: [\n            '民间文学',\n            '传统音乐',\n            '传统舞蹈',\n            '传统戏剧',\n            '曲艺',\n            '传统体育、游艺与杂技',\n            '传统美术',\n            '传统技艺',\n            '传统医药',\n            '民俗',\n        ], // 标识的文字内容，应与 series 中的 name 一致\n        bottom: 10, // 设置legend显示在底部\n        textStyle: {\n            color: '#08544c', // 文字颜色\n            fontSize: 10, // 文字大小\n        },\n        },\n\n        //鼠标滑过提示框显示\n        tooltip: {\n          backgroundColor: \"rgba(0,0,0,0.7)\", //背景颜色\n          // borderWidth: 0,// 提示框得边框\n          textStyle: {\n            color: \"#fff\",\n            fontSize: 10,\n          },\n          formatter: function (params) {\n            //在此处直接用 formatter 属性\n            // 这里可以使用html自定义自己想要得样式\n            return `<div>${params.name}<a style=\"color:#1089E7\">${params.value}</a>项</div>\n                    `;\n          },\n        },\n        dataZoom: [\n          {\n            id: \"dataZoomX\",\n            show: false,\n            type: \"slider\",\n            xAxisIndex: [0],\n            filterMode: \"filter\",\n            startValue: 0,\n            endValue: 4,\n          },\n        ],\n        xAxis: {\n            data: [\n            '民间文学',\n            '传统音乐',\n            '传统舞蹈',\n            '传统戏剧',\n            '曲艺',\n            '传统体育、游艺与杂技',\n            '传统美术',\n            '传统技艺',\n            '传统医药',\n            '民俗',\n        ],\n          axisTick: { show: false },\n          axisLine: { show: false },\n          axisLabel: {\n            show: false,\n            color: \"#08544c\",\n            fontSize: 10,\n          },\n        },\n        yAxis: {\n          splitLine: { show: true },\n          axisTick: { show: false },\n          axisLine: { show: false },\n          axisLabel: {\n            show: true,\n            color: \"#08544c\",\n            fontSize: 10,\n          },\n        },\n        color: [\"#34bca3\"],\n        series: [\n          {\n            name: \"hill\",\n            type: \"pie\",\n            radius: [\"40%\", \"55%\"],\n            center: [\"50%\", \"50%\"],\n            barCategoryGap: \"-130%\",\n            // symbol: 'path://M0,10 L10,10 L5,0 L0,10 z',\n            symbol:\n              \"path://M0,10 L10,10 C5.5,10 5.5,5 5,0 C4.5,5 4.5,10 0,10 z\",\n            labelLine: {\n              normal: {\n                show: false, // show设置线是否显示，默认为true，可选值：true ¦ false\n              },\n            },\n            itemStyle: {\n              color: function (param) {\n                return myColor[param.dataIndex];\n              },\n              opacity: 0.5,\n            },\n            emphasis: {\n              itemStyle: {\n                color: \"#34bca3\",\n                opacity: 1,\n              },\n            },\n            label: {\n            show: false, // 将此处的show属性设置为false\n        },\n            data: SData,\n          },\n        ],\n      };\n      myChart.clear();\n      myChart.setOption(option, true);\n      window.addEventListener(\"resize\", function () {\n        myChart.resize();\n      });\n    })();\n\n    //地图\n    (function () {\n      var chartDom = document.getElementById(\"main\");\n      var myChart = echarts.init(chartDom);\n      echarts.registerMap(\"liaoning\", sichuanJson);\n\n      var option = {\n        tooltip: {\n          //提示框\n          trigger: \"item\",\n          backgroundColor: \"rgba(200, 229, 218, 0.4)\",\n          borderWidth: 0,\n          show: true,\n          enterable: true,\n          formatter: function (params) {\n            console.log(params);\n\n            return `<div style=\"width:180px; height:max-content;\">\n                        <div style=\"display: inline-block; text-align:center; width:100%; color: #08544c; font-size: 10px\">${params.name}</div>\n\n\n                        </div>\n                        `;\n          },\n        },\n        series: [\n          {\n            type: \"map\",\n            map: \"liaoning\",\n            show: true,\n            roam: false,\n            label: {\n              normal: {\n                show: false,\n              },\n              emphasis: {\n                show: true,\n                fontSize: 14,\n                color: \"#030303\",\n              },\n            },\n            itemStyle: {\n              normal: {\n                borderWidth: 0.7,\n                areaColor: \"#ADD8E6\",\n                borderColor: \"#fff\", // 线\n              },\n              emphasis: {\n                // 也是选中样式\n                borderWidth: 2,\n                borderColor: \"#fff\",\n                areaColor: \"#1E90FF\", //悬浮区背景\n              },\n            },\n            animation: true,\n          },\n        ],\n      };\n      myChart.setOption(option);\n    })();\n  },\n};\n</script>\n\n<style scoped>\n\n.huapian_item{\n    width: 1500px !important;\n    padding: 20px !important;\n    margin: 0 0 !important;\n\n\n}\n.huapian_item p{\n    box-sizing: border-box !important;\n    margin-right: 20px !important;\n\n}\n.map .info {\n  text-align: left;\n  bottom: 20px;\n  margin-left: 50px;\n  margin-top: -60px;\n  right: 40px;\n  font-size: 17px;\n  color: #535353 !important;\n}\n.bgClass {\n    position: relative; \n\n}\n\n.bgClass::before {\n    content: \"\";\n    background-image: url(\"../../images/gushi.png\");\n    opacity: 0.3; \n    position: absolute;\n    background-repeat: no-repeat;\n\n    background-size: contain;\n    top: 0;\n    left: 0;\n    width: 100%;\n    height: 100%;\n\n}\n.wrapper {\n  position: relative;\n  background-image: url(\"../../assets/222.png\");\n  background