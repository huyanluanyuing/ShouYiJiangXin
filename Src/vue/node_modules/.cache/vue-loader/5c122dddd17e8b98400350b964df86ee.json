n-top: 10px; margin-bottom: 20px; padding-left: 0; width: 320px;\"\r\n              size=\"medium\" prefix-icon=\"el-icon-lock\" show-password v-model=\"user.password\"></el-input>\r\n          </el-form-item>\r\n\r\n          <router-link :to=\"{ name: 'Prepare' }\">\r\n          <el-button class=\"primary-btn\" @click=\"login\">立即登录</el-button>\r\n          </router-link>\r\n          <!-- 将立即注册按钮放在此处 -->\r\n          <el-button class=\"primary-btn\" @click=\"isLogin = !isLogin\">立即注册</el-button>\r\n        </el-form>\r\n      </div>\r\n\r\n      <div :class=\"['container', 'container-register', { 'is-txl': isLogin }]\">\r\n        <el-form class=\"form\" :model=\"user\" :rules=\"rules\" ref=\"userForm\">\r\n\r\n          <h2 class=\"title\">注册页面</h2>\r\n\r\n\r\n          <el-form-item prop=\"usertype\">\r\n            <el-select v-model=\"value\" placeholder=\"请选择身份\" style=\"margin-top: 5px; padding-left: 0; width: 320px;\"\r\n              size=\"medium\" @change=\"handleSelectChange\" prefix-icon=\"el-icon-user\">\r\n              <el-option v-for=\"item in options\" :key=\"item.value\" :label=\"item.label\" :value=\"item.value\">\r\n              </el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n\r\n          <el-form-item prop=\"username\">\r\n            <el-input placeholder=\"请输入账号\" style=\"margin-top: 3px; padding-left: 0; width: 320px;\" size=\"medium\"\r\n              prefix-icon=\"el-icon-user\" v-model=\"user.username\"></el-input>\r\n          </el-form-item>\r\n\r\n          <el-form-item prop=\"password\">\r\n            <el-input placeholder=\"请输入密码\" style=\"margin-top: 3px; padding-left: 0; width: 320px;\" size=\"medium\"\r\n              prefix-icon=\"el-icon-lock\" show-password v-model=\"user.password\"></el-input>\r\n          </el-form-item>\r\n\r\n          <el-form-item prop=\"confirmPassword\">\r\n            <el-input placeholder=\"请确认密码\" style=\"margin-top: 3px; padding-left: 0; width: 320px;\" size=\"medium\"\r\n              prefix-icon=\"el-icon-lock\" show-password v-model=\"user.confirmPassword\"></el-input>\r\n          </el-form-item>\r\n\r\n          <div class=\"button-container\">\r\n            <el-button class=\"primary-btn\" @click=\"register\">立即注册</el-button>\r\n            <el-button class=\"primary-btn\" @click=\"isLogin = !isLogin\">返回</el-button>\r\n          </div>\r\n\r\n\r\n        </el-form>\r\n      </div>\r\n\r\n\r\n    </div>\r\n\r\n\r\n  </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport request from \"@/utils/request\";\r\nimport \"../fonts/FZQKBYSJW.css\";\r\n// import { setRoutes } from \"@/router\";\r\n\r\nexport default {\r\n  name: \"Login\",\r\n  data() {\r\n    return {\r\n      isLogin: true,\r\n      radio: 1,\r\n      options: [{\r\n        value: '1',\r\n        label: '普通用户'\r\n      }, {\r\n        value: '2',\r\n        label: '管理员'\r\n      }],\r\n      value: '',\r\n      user: {},\r\n      rules: {\r\n        username: [\r\n          { required: true, message: '请输入用户名', trigger: 'blur' },\r\n          { min: 3, max: 10, message: '长度在 3 到 5 个字符', trigger: 'blur' }\r\n        ],\r\n        password: [\r\n          { required: true, message: '请输入密码', trigger: 'blur' },\r\n          { min: 1, max: 20, message: '长度在 1 到 20 个字符', trigger: 'blur' }\r\n        ],\r\n        confirmPassword: [\r\n          { required: false, message: '请输入密码', trigger: 'blur' },\r\n          { min: 1, max: 20, message: '长度在 1 到 20 个字符', trigger: 'blur' }\r\n        ],\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    login() {\r\n      request.post(\"/user/loginUser\", this.user).then(res => {\r\n        if (res.code === '200') {\r\n          if (res.data.role_id === 100) {\r\n            localStorage.setItem(\"username\", JSON.stringify(res.data.username))\r\n            localStorage.setItem(\"role\", JSON.stringify('管理员'))\r\n            this.$router.push(\"/admin\")\r\n            this.$message.success(\"登录成功\")\r\n          }\r\n          else if (res.data.role_id === 200) {\r\n            localStorage.setItem(\"username\", JSON.stringify(res.data.username))\r\n            localStorage.setItem(\"role\", JSON.stringify('用户'))\r\n            this.$router.push(\"/home\")\r\n            this.$message.success(\"登录成功\")\r\n          }\r\n        } else {\r\n          this.$message.error(\"用户名或密码错误\")\r\n        }\r\n      });\r\n    },\r\n    register() {\r\n      if (this.radio === 1) {\r\n        request.post(\"/user/RegisterClientUser\", this.user).then(res => {\r\n          if (res.code === '200') {\r\n            localStorage.setItem(\"user\", JSON.stringify(res.data))\r\n            this.$router.push(\"/login\")\r\n            this.$message.success(\"用户注册成功\")\r\n          } else {\r\n            this.$message.error(\"注册失败\")\r\n          }\r\n        }\r\n        )\r\n      } else if (this.radio === 2) {\r\n        request.post(\"/user/RegisterAdminUser\", this.user).then(res => {\r\n          if (res.code === '200') {\r\n            localStorage.setItem(\"user\", JSON.stringify(res.data))\r\n            this.$router.push(\"/login\")\r\n            this.$message.success(\"管理员注册成功\")\r\n          } else {\r\n            this.$message.error(\"注册失败\")\r\n          }\r\n        })\r\n      }\r\n    },\r\n    handleSelectChange(value) {\r\n      switch (value) {\r\n        case '1':\r\n          this.radio = 1\r\n          console.log(this.radio)\r\n          break;\r\n        case '2':\r\n          this.radio = 2\r\n          console.log(this.radio)\r\n          break;\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\nbody {\r\n  margin: 0;\r\n  padding: 0;\r\n}\r\n\r\n.wrapper {\r\n  position: relative;\r\n  height: 100vh;\r\n  background-image: url(../assets/bg.png);\r\n  background-size: 110%;\r\n  overflow: hidden;\r\n}\r\n\r\n.w {\r\n  width: 1210px;\r\n  margin: auto;\r\n}\r\n\r\n.header {\r\n  height: 30px;\r\n\r\n  margin: 0px auto;\r\n  margin-top: 30px;\r\n}\r\n\r\n.logo {\r\n  float: left;\r\n  width: 60px;\r\n  height: 60px;\r\n  margin-top: -13px;\r\n  background-image: url(../assets/logo.png);\r\n  background-repeat: no-repeat;\r\n  background-position-x: center;\r\n  background-size: 100%;\r\n}\r\n\r\n.name {\r\n  float: left;\r\n  font-size: 25px;\r\n  line-height: 30px;\r\n}\r\n\r\n.nav {\r\n  float: right;\r\n  margin-left: 60px;\r\n}\r\n\r\n.nav ul li {\r\n  float: left;\r\n  margin: 0 15px;\r\n  list-style: none;\r\n}\r\n\r\n.nav ul li a {\r\n  display: inline-block;\r\n  text-decoration: none;\r\n  height: 30px;\r\n  line-height: 30px;\r\n  padding: 0 10px;\r\n  font-size: 18px;\r\n  color: #343434;\r\n}\r\n\r\n.nav ul li a:hover {\r\n  border-bottom: 2px solid #030303;\r\n  color: #030303;\r\n}\r\n\r\n.slogan {\r\n  margin: 10px auto;\r\n  margin-bottom: 35px;\r\n  width: 750px;\r\n  height: 150px;\r\n  background-image: url(\"../assets/slogan.png\");\r\n  background-size: 100%;\r\n}\r\n\r\n.main-box {\r\n  position: relative;\r\n  margin: 0 auto;\r\n  margin-top: 80px;\r\n  width: 520px;\r\n  min-width: 520px;\r\n  height: 400px; /* 调整高度为 400px 或其他你想要的值 */\r\n  padding: 20px;\r\n  background-image: linear-gradient(to bottom right, rgba(226, 240, 236, 0, 1), rgba(246, 239, 240, 0.1));\r\n  overflow: hidden;\r\n}\r\n\r\n\r\n.container {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  position: absolute;\r\n  top: 0;\r\n  width: 500px;\r\n  height: 100%;\r\n  padding: 20px;\r\n  background-color: #EDF5F5;\r\n\r\n  transition: opacity 0.5s ease-in-out;\r\n}\r\n\r\n.form {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  flex-direction: column;\r\n  width: 100%;\r\n  height: 100%;\r\n  color: #a0a5a8;\r\n}\r\n\r\n.form .title {\r\n  font-size: 32px;\r\n  font-weight: 700;\r\n  line-height: 2;\r\n  color: #181818;\r\n}\r\n\r\n.form .text {\r\n  font-size: 14px;\r\n  margin-top: 18px;\r\n  margin-bottom: 24px;\r\n}\r\n\r\n.container-register {\r\n  z-index: 100;\r\n  left: calc(100% - 520px);\r\n}\r\n\r\n.container-login {\r\n  left: calc(100% - 520px);\r\n  z-index: 0;\r\n}\r\n\r\n.is-txl {\r\n  left: 0;\r\n  transition: 1.25s;\r\n  opacity: 1;\r\n}\r\n\r\n.is-z200 {\r\n  z-index: 200;\r\n  transition: 1.25s;\r\n}\r\n\r\n\r\n\r\n.switch .switch__circle {\r\n  position: absolute;\r\n  width: 240px;\r\n  height: 240px;\r\n  border-radius: 50%;\r\n  background-color: #EDF5F5;\r\n  box-shadow: inset 8px 8px 12px #d1d9e6, inset -8px -8px 12px #f9f9f9;\r\n  left: -20%;\r\n  bottom: -30%;\r\n  transition: 1.25s;\r\n}\r\n\r\n.switch .switch__circle_top {\r\n  top: -25%;\r\n  left: 70%;\r\n  width: 200px;\r\n  height: 200px;\r\n}\r\n\r\n.switch .switch__container {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  flex-direction: column;\r\n  position: absolute;\r\n  width: 360px;\r\n  padding: 50px auto;\r\n  padding-bottom: 55px;\r\n  transition: 1.25s;\r\n}\r\n\r\n.switch .switch__container h2 {\r\n  font-size: 30px;\r\n  font-weight: 700;\r\n  line-height: 3;\r\n  color: #181818;\r\n}\r\n\r\n.switch .switch__container p {\r\n  margin-top: 10px;\r\n  font-size: 13px;\r\n  letter-spacing: 0.25px;\r\n  text-align: center;\r\n  line-height: 1.6;\r\n}\r\n\r\n.login {\r\n  left: calc(100% - 360px);\r\n}\r\n\r\n.login .switch__circle {\r\n  left: 0;\r\n}\r\n\r\n.primary-btn {\r\n  width: 150px;\r\n  height: 40px;\r\n  /* 移除 border-radius 属性 */\r\n  /* border-radius: 20px; */\r\n  margin-top: 20px;\r\n  text-align: center;\r\n  font-size: 14px;\r\n  letter-spacing: 2px;\r\n  background-color: #bc3454;\r\n  color: #f9f9f9;\r\n  cursor: pointer;\r\n  box-shadow: 8px 8px 16px #d1d9e6, -5px -5px 10px #f9f9f9;\r\n  /* 添加边框属性 */\r\n  border: 2px solid #bc3454;\r\n}\r\n\r\n.primary-btn:hover {\r\n  box-shadow: 2px 2px 6px 0 rgba(255, 255, 255, 0.6),\r\n  -2px -2px 6px 0 rgba(116, 125, 136, 0.5),\r\n  inset -4px -4px 3px 0 rgba(255, 255, 255, 0.2),\r\n  inset 4px 4px 3px 0 rgba(0, 0, 0, 0.4);\r\n  /* 移除原有的边框属性 */\r\n  border: none;\r\n}\r\n\r\n.font_fzqkbysjt {\r\n  font-family: STKAITI;\r\n  font-weight: bold;\r\n  color: white; /* 将字体颜色设置为白色 */\r\n}\r\n.title {\r\n  font-family: '华文楷体', cursive;\r\n  font-weight: bold;\r\n}\r\n</style>\r\n"]}]}