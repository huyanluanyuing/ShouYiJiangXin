        // 每隔一段时间更新当前显示的数据项索引\n        this.currentIndex = (this.currentIndex + 1) % this.itemCount;\n      }, 3000); // 3秒滚动一次，可以根据需要调整时间间隔\n    },\n\n    handleClick_xihuan(tab, event) {\n      console.log(tab, event);\n    },\n    handleSizeChange(val) {\n      console.log(`每页 ${val} 条`);\n    },\n    handleCurrentChange(val) {\n      console.log(`当前页: ${val}`);\n    },\n    next() {\n      // 向下滚动一页\n      this.$refs.fullpage.api.moveSectionDown();\n    }\n  }\n}\n</script>\n\n<style scoped>\n.wrapper {\n  position: relative;\n  background-image: url(\"../../assets/222.png\");\n  background-size: 100%;\n  height: 180vh;\n  background-size: cover; /* 将背景图放大以覆盖整个容器 */\n  overflow: hidden;\n}\n.midtitle{\n  font-family: '华文楷体';\n  font-size: 30px;\n  color: #7fac9b;\n  font-weight: bold;\n  text-align: left;\n  margin-top:0px;\n}\n.leftList .title{\n  font-family: '华文楷体';\n  font-size: 30px;\n  color: #7fac9b;\n  font-weight: bold;\n  text-align: center;\n}\n.righttitle{\n  font-family: '华文楷体';\n  font-size: 30px;\n  color: #7fac9b;\n  font-weight: bold;\n  text-align: center;\n  margin-bottom: 10px;\n\n  margin-top: 10px;\n}\n .seamless-warp {\n        height: 1000px;\n        overflow: hidden;\n    }\n.slide-right-enter-active, .slide-right-leave-active {\n  transition: all 1s; /* 过渡时间可以根据需要调整 */\n}\n.slide-right-enter, .slide-right-leave-to {\n  transform: translateX(100%);\n}\n.right{\n  /* background: red; */\n  width: 100%;\n  /* height:1000px; */\n  /* overflow: hidden; */\n}\n.right .image-card{\n  display: inline-block;\n  box-sizing: border-box;\n  width: 100%;\n  /* height:200px; */\n  padding: 10px;\n}\n.right .image-card img{\n  width: 100%;\n  height: 300px;\n  object-fit: cover;\n  border-radius: 10px;\n}\n.right .card-text{\n  color: rgb(47, 47, 47);\n  font-size: 18px;\n  font-family: '华文楷体', cursive;\n  text-align: center; /* 水平居中文本 */\n}\n.card-text-up{\n  color: rgb(35, 34, 34);\n  font-size: 10px;\n  margin-bottom:5px;\n  font-family: '华文楷体', cursive;\n}\n.leftList{\n  width: 100%;\n  /* background: red; */\n}\n.leftList .image-card{\n  display: inline-block;\n  box-sizing: border-box;\n  width: 50%;\n  height:200px;\n  padding: 10px;\n}\n.leftList .image-card img{\n  width: 100%;\n  height: 70%;\n  object-fit: cover;\n  border-radius: 5px;\n}\n.leftList .card-text{\n  color: rgb(47, 47, 47);\n  font-size: 18px;\n  font-family: '华文楷体', cursive;\n  text-align: center; /* 水平居中文本 */\n}\n* {\n    margin: 0;\n    padding: 0;\n}\n\n.font_fzqkbysjt {\n    //font-family: fzqkbysjt;\n    font-family: '华文楷体', cursive;\n}\n\n.mainbox {\n    display: flex;\n    min-width: 1024px;\n    max-width: 1920px;\n    margin: 0px auto;\n    padding: 0;\n}\n\n.mainbox .column {\n    flex: 3;\n}\n\n.mainbox .column:nth-child(2) {\n    flex: 5;\n    margin: 0 0.125rem 0.1875rem;\n    overflow: hidden;\n}\n\n.mainbox .panel {\n    position: relative;\n    height: 3.95rem;\n    padding: 2px;\n    margin: 0 5px 10px 5px;\n    border: 1px solid rgba(25, 186, 139, 0.17);\n}\n\n.mainbox .panel::before {\n    position: absolute;\n    top: 0;\n    left: 0;\n    width: 10px;\n    height: 10px;\n    border-left: 2px solid #02a6b5;\n    border-top: 2px solid #02a6b5;\n    content: \"\";\n}\n\n.mainbox .panel::after {\n    position: absolute;\n    top: 0;\n    right: 0;\n    width: 10px;\n    height: 10px;\n    border-right: 2px solid #02a6b5;\n    border-top: 2px solid #02a6b5;\n    content: \"\";\n}\n\n.mainbox .panel-footer {\n    position: absolute;\n    bottom: 0;\n    left: 0;\n    width: 100%;\n}\n\n.mainbox .panel-footer::before {\n    position: absolute;\n    left: 0;\n    bottom: 0;\n    width: 10px;\n    height: 10px;\n    border-left: 2px solid #02a6b5;\n    border-bottom: 2px solid #02a6b5;\n    content: \"\";\n}\n\n.mainbox .panel-footer::after {\n    position: absolute;\n    right: 0;\n    bottom: 0;\n    width: 10px;\n    height: 10px;\n    border-right: 2px solid #02a6b5;\n    border-bottom: 2px solid #02a6b5;\n    content: 